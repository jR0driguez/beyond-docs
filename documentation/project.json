{
  "version": 1,
  "scope": "beyond",
  "name": "docs",
  "title": "Beyond Docs",
  "description": "Beyond documentation site",
  "template": "template/template.json",
  "backend": {
  },
  "languages": {
    "default": "en",
    "supported": [
      "en",
      "es"
    ]
  },
  "params": {
    "logs": "./logs",
    "backend": {
      "test1": "test2"
    },
    "web": {
      "test2": "test2"
    }
  },
  "modules": {
    "path": "./modules",
    "externals": {
      "slate": {
        "development": "dist/slate.js",
        "production": "dist/slate.min.js"
      },
      "slate-react": {
        "development": "dist/slate-react.js",
        "production": "dist/slate-react.min.js"
      },
      "escape-html": {
        "development": "index.js",
        "production": "index.js"
      },
      "is-hotkey": {
        "development": "src/index.js",
        "production": "src/index.js"
      },
      "prop-types": {
        "development": "prop-types.js",
        "production": "prop-types.min.js"
      },
      "perfect-scrollbar": {
        "development": "dist/perfect-scrollbar.js",
        "production": "dist/perfect-scrollbar.js"
      }
    }
  },
  "deployment": {
    "distributions": [
      {
        "name": "web",
        "platform": "web",
        "port": 3500,
        "default": true,
        "environment": "development",
        "backend": "backend",
        "imports": {
          "@beyond/ui": "web",
          "@beyond-js/kernel": "web",
          "@beyond-js/widgets": "web",
          "@beyond-js/react-widgets": "web",
          "@beyond-js/vue-widgets": "web",
          "@beyond-js/svelte-widgets": "web",
          "@beyond-js/backend": "web",
          "@beyond-js/ssr": "web"
        }
      },
      {
        "name": "backend",
        "platform": "backend",
        "port": 4600,
        "environment": "development"
      },
      {
        "name": "web-tsc",
        "platform": "web",
        "port": 4700,
        "environment": "development",
        "ts": {
          "compiler": "tsc"
        }
      }
    ]
  },
  "libraries": {
    "imports": [
      "@beyond/ui",
      "@beyond-js/kernel",
      "@beyond-js/widgets",
      "@beyond-js/react-widgets",
      "@beyond-js/vue-widgets",
      "@beyond-js/svelte-widgets",
      "@beyond-js/backend",
      "@beyond-js/ssr"
    ]
  },
  "static": {
    "includes": [
      "./images"
    ]
  }
}
