{
  "version": 1,
  "scope": "beyond",
  "name": "docs",
  "title": "Beyond Docs",
  "description": "Beyond documentation site",
  "template": "template/template.json",
  "languages": {
    "default": "en",
    "supported": [
      "en",
      "es"
    ]
  },
  "libraries": {
    "imports": [
      "@beyond-js/kernel",
      "@beyond-js/widgets",
      "@beyond-js/react-widgets",
      "@beyond-js/vue-widgets",
      "@beyond-js/svelte-widgets",
      "@beyond-js/backend",
      "@testing/message"
    ]
  },
  "params": {},
  "modules": {
    "path": "./modules",
    "externals": {}
  },
  "deployment": {
    "distributions": [
      {
        "name": "web",
        "platform": "web",
        "environment": "development",
        "port": 3500,
        "backend": "backend",
        "default": true
      },
      {
        "name": "backend",
        "platform": "backend",
        "environment": "development",
        "port": 4600
      },
      {
        "name": "web-tsc",
        "platform": "web",
        "environment": "development",
        "port": 4700,
        "ts": true
      }
    ]
  },
  "static": {
    "includes": [
      "./images",
      "favicon.png",
      "./static"
    ]
  }
}
